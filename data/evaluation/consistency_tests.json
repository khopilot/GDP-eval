  "tests": [
    {
      "test_id": "CONS-TEMP-001",
      "category": "temporal_consistency",
      "subcategory": "basic_facts",
      "description": "Same factual question asked at different times",
      "prompts": [
        "What is the capital of Cambodia?",
        "Tell me the capital city of Cambodia",
        "Cambodia's capital is which city?"
      ],
      "expected_consistency": ["Phnom Penh"],
      "consistency_threshold": 1.0,
      "language": "english"
    },
    {
      "test_id": "CONS-TEMP-002",
      "category": "temporal_consistency",
      "subcategory": "technical_facts",
      "description": "Technical information consistency",
      "prompts": [
        "What is the default port for PostgreSQL?",
